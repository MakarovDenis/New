using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace HomeWork4_3_
{
    class Program
    {
        static void Main(string[] args)
        {
            int a;
          m:  Console.WriteLine("Введите число от 0 до 100");
            a = Convert.ToInt32(Console.ReadLine());

            if (a >= 0 && a <= 14) { 
           
                Console.WriteLine("Число входит в промежуток [0-14]");
            }
            else if (a >= 15 && a <= 35) {
                Console.WriteLine("Число входит в промежуток [15-35]");
            }
            else if (a>=36 && a <= 50){
            
                Console.WriteLine("Число входит в промежуток [36-50]");
            }
            else if (a>=51 && a <= 100)
            {
                Console.WriteLine("Число входит в промежуток [51-100]");
            }

            else
            {
                Console.WriteLine("Число не входит не в один из промежутков");
            }

            Console.ReadKey();
            goto m;
        }
       

    }

}
